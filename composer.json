{
	"name": "laravel/laravel",
	"description": "The Laravel Framework.",
	"keywords": ["framework", "laravel"],
	"require": {
		"laravel/framework": "4.1.*",
		"laravelbook/ardent": "2.4.*",
		"frozennode/administrator": "4.12.*",
		"doctrine/dbal" : "2.4.*"
	},
	"require-dev" : {
		"phpunit/phpunit": "3.7.*",
		"mockery/mockery": "0.9.*",
		"squizlabs/php_codesniffer": "2.1.*"
	},
	"autoload": {
		"classmap": [
			"app/commands",
			"app/controllers",
			"app/models",
			"app/database/migrations",
			"app/database/seeds",
			"app/tests/TestCase.php"
		],
        "psr4" : {
            "Judge\\" : "app/Judge"
        }
	},
	"scripts": {
		"post-install-cmd": [
			"php setup.php"
		],
		"pre-update-cmd": [
			"php setup.php"
		],
		"post-update-cmd": [
			"php setup.php"
		],
		"post-create-project-cmd": [
			"php artisan key:generate"
		]
	},
	"config": {
		"preferred-install": "dist"
	},
	"minimum-stability": "dev"
}
